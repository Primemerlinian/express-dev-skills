<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Express-Skills-Lab</title>
</head>
<body>
  <h1>Skills</h1>
  <ul>
    <% skills.forEach(function(skills) { %>
      <li>
				<a href="/skills/<%= skills._id %>"> <%= skills.text %></a>
				- 
				<%= skills.done ? 'done' : 'not done' %>
			</li>
    <% }) %>
  </ul>
  <a href="/skills/new">Add Skills</a>
</body>
</html>